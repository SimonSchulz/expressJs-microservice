{
  "name": "nodanorm-new-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon -L ./src/index.ts",
    "tsc": "tsc",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "migration:create": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:create -n ",
    "migration:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -n ",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -c ",
    "migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert -c ",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "prepare": "husky install",
    "schema:drop": "ts-node --transpile-only ./node_modules/typeorm/cli.js schema:drop -c "
  },
  "repository": {
    "type": "git",
    "url": "git@git.andersenlab.com/Andersen/nodanorm-new/user-service"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "ts-node-dev": "^1.1.8",
    "typeorm": "^0.2.45",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
